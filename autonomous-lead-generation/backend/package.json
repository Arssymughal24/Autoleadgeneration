{
  "name": "autonomous-lead-generation-engine",
  "version": "1.0.0",
  "description": "Fully autonomous lead generation engine with GPT-4 AI, campaign analytics, A/B testing, and custom scoring algorithms",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "queue:dashboard": "node scripts/queue-dashboard.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:prod": "docker-compose up"
  },
  "keywords": [
    "lead-generation",
    "ai",
    "automation",
    "crm",
    "salesforce",
    "gpt-4",
    "analytics",
    "ab-testing"
  ],
  "author": "Autonomous Lead Generation Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsforce": "^2.0.0-beta.29",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.20.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "simple-statistics": "^7.8.3"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}